<!DOCTYPE html>
<html lang="en">
<head>
  <title>TinyApp</title>
  <% include ./partials/_headers %>
</head>
<body>
  <h2>Welcome to Tiny App</h2>
    <% if(urls) { %>
      <% Object.keys(urls).forEach(function(url) { %>
          <li><%= url %> -> <%= urls[url]%> <a href = "/urls/<%= url %>"> Edit </a>
          <form method="POST" action = '/urls/<%= url %>/delete'>
            <input type="submit" value="Delete">
          </form></li>
      <% }); %>
    <% } else { %>
    <p> No URLS for you </p>
    <% } %>
</body>
</html>